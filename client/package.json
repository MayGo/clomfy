{
  "name": "clomfy",
  "version": "1.0.0",
  "engines": {
    "npm": ">=3"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": ["app"],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": ["istanbul"]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": ["global"]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": ["composes"]
        }
      ],
      "indentation": 2
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": ["core-js", "lodash", "eventsource-polyfill"]
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "balloon-css": "^0.5.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "^4.0.0",
    "enzyme-to-json": "^3.1.1",
    "express": "^4.15.2",
    "fontfaceobserver": "^2.0.9",
    "history": "^2.0.45",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "immutable": "3.8.1",
    "invariant": "^2.2.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "moment": "^2.18.1",
    "npm": "^5.3.0",
    "query-string": "^5.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-i18next": "^4.6.4",
    "react-intl": "^2.3.0",
    "react-json-view": "^1.12.1",
    "react-materialui-notifications": "^0.5.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "redux": "3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "0.15.4",
    "redux-saga-routines": "^1.1.2",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.0.2",
    "typeface-berkshire-swash": "^0.0.31",
    "typescript": "2.4.1",
    "warning": "3.0.0",
    "whatwg-fetch": "^2.0.3",
    "with-query": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/chai-enzyme": "^0.6.1",
    "@types/enzyme": "^2.7.7",
    "@types/express": "^4.0.35",
    "@types/fontfaceobserver": "0.0.5",
    "@types/history": "3.2.1",
    "@types/invariant": "^2.2.28",
    "@types/lodash": "^4.14.61",
    "@types/mocha": "^2.2.40",
    "@types/react": "15.0.38",
    "@types/react-dom": "15.5.1",
    "@types/react-intl": "2.3.1",
    "@types/react-redux": "4.4.46",
    "@types/react-router": "3.0.12",
    "@types/react-router-redux": "4.0.48",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "3.0.38",
    "@types/redux-devtools-dock-monitor": "1.1.31",
    "@types/redux-devtools-log-monitor": "1.0.31",
    "@types/redux-immutable": "3.0.34",
    "@types/sinon": "^2.1.2",
    "@types/systemjs": "^0.20.2",
    "@types/webpack": "^2.2.14",
    "awesome-typescript-loader": "^3.2.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "expect-jsx": "^3.0.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.0",
    "imports-loader": "^0.7.1",
    "json-loader": "0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^2.0.3",
    "lint-staged": "^3.4.0",
    "material-ui": "v1.0.0-beta.13",
    "material-ui-icons": "^1.0.0-beta.15",
    "material-ui-pagination": "^1.1.6",
    "mocha": "^3.2.0",
    "ngrok": "^2.2.6",
    "nps": "^5.4.0",
    "nps-utils": "^1.2.0",
    "null-loader": "0.1.1",
    "offline-plugin": "^4.7.0",
    "plop": "^1.7.4",
    "postcss-cssnext": "^2.10.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "psi": "^3.0.0",
    "react-addons-test-utils": "~15.4.2",
    "react-hot-loader": "next",
    "redux-form": "^7.0.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.7",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "timeago-react": "^1.2.2",
    "tsconfig-paths": "^2.1.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/common/store.ts"
    ],
    "moduleFileExtensions": ["js", "json", "ts", "tsx"],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-tests.ts",
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "transform": {
      ".tsx?": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "**/__tests__/**/*.ts?(x),**/?(*.)(spec|test).ts?(x)"
  }
}
